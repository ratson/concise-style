{
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "globalReturn": true,
      "jsx": true
    },
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": ["babel"],
  "rules": {
    "babel/object-curly-spacing": ["error", "always"],
    "object-curly-spacing": "off",
    "strict": "off"
  }
}

